<%--
    Show an individual project stage
--%>
<div class="row">
    <div class="span12">
        <span class="icon-plus" ></span>
        <span>${stage.type.name} </span>
        <span>${stage.status} </span>
        <span>${stage.createdTime} </span>
        <span>${stage.startedTime} </span>
        <span>${stage.completedTime} </span>
        <span class="icon-resize-vertical"></span>
    </div>
</div>