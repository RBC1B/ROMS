<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping  package="uk.org.rbc1b.roms.db.volunteer.qualification">
    <class name="Qualification" table="Qualification">
        <id name="qualificationId" >
            <generator class="identity"/>
        </id>
        <property name="name" length="50" />
        <property name="description" length="150" />
        <property name="updateTime" not-null="true"  />
        <property name="updatedBy" not-null="true"  />
    </class>
</hibernate-mapping>
